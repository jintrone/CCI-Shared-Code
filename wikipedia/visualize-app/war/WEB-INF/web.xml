<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WikipediaUsertalkViz</servlet-name>
		<servlet-class>edu.mit.cci.wikipedia.vizservlet.WikipediaUsertalkVizServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WikipediaUsertalkViz</servlet-name>
		<url-pattern>/utviz</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>WikipediaEditViz</servlet-name>
		<servlet-class>edu.mit.cci.wikipedia.vizservlet.WikipediaEditVizServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WikipediaEditViz</servlet-name>
		<url-pattern>/editviz</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>WikipediaMultiUsertalkViz</servlet-name>
		<servlet-class>edu.mit.cci.wikipedia.vizservlet.WikipediaMultiUsertalkVizServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WikipediaMultiUsertalkViz</servlet-name>
		<url-pattern>/mutviz</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>WikipediaMultiEditViz</servlet-name>
		<servlet-class>edu.mit.cci.wikipedia.vizservlet.WikipediaMultiEditVizServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WikipediaMultiEditViz</servlet-name>
		<url-pattern>/meditviz</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>WikipediaStats</servlet-name>
		<servlet-class>edu.mit.cci.wikipedia.vizservlet.StatServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WikipediaStats</servlet-name>
		<url-pattern>/statsviz</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>WikipediaRanking</servlet-name>
		<servlet-class>edu.mit.cci.wikipedia.vizservlet.GetRankingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WikipediaRanking</servlet-name>
		<url-pattern>/ranklist</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
